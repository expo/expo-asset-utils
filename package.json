{
  "name": "expo-asset-utils",
  "version": "1.2.0",
  "description": "Utilities for converting files into Expo Assets",
  "author": "Evan Bacon <bacon@expo.io> (https://expo.io/)",
  "files": [
    "src"
  ],
  "homepage": "https://github.com/expo/expo-asset-utils#readme",
  "readmeFilename": "README.md",
  "license": "MIT",
  "main": "src/index.js",
  "keywords": [
    "expo",
    "asset",
    "utils",
    "react-native",
    "react"
  ],
  "scripts": {
    "jest": "jest",
    "test-update-snapshot": "jest --updateSnapshot",
    "lint": "eslint .",
    "format": "eslint --fix .",
    "precommit": "lint-staged",
    "test": "npm run lint && npm run jest",
    "sync-example": "rsync -rv src example/node_modules/expo-asset-utils/; rsync -rv package.json example/node_modules/expo-asset-utils/;"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "notify": true,
    "preset": "jest-expo",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/example/"
    ],
    "testRegex": "/__tests__/[^/]+-test\\.js$"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/expo/expo-asset-utils.git"
  },
  "bugs": {
    "url": "https://github.com/expo/expo-asset-utils/issues"
  },
  "directories": {
    "example": "example",
    "lib": "src"
  },
  "peerDependencies": {
    "@unimodules/core": "*",
    "expo-asset": "~4.0.0",
    "expo-file-system": "~4.0.0",
    "expo-font": "~4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^22.4.1",
    "babel-preset-react-native": "^2.1.0",
    "codecov": "^3.6.5",
    "conventional-changelog-cli": "^2.0.5",
    "eslint": "^4.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.138.0",
    "husky": "^0.14.3",
    "jest": "^22.1.3",
    "jest-expo": "^25.1.0",
    "lint-staged": "^4.2.1",
    "prettier": "^1.12.1",
    "prettier-eslint": "^8.8.1",
    "react": "16.2.0",
    "react-native": "^0.52.0",
    "react-native-vector-icons": "^4.2.0",
    "react-test-renderer": "^16.0.0",
    "release-it": "^7.6.1"
  },
  "eslintConfig": {
    "extends": "expo"
  }
}
